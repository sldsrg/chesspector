{
	"env": {
		"jasmine": true
	},  
	"extends": [
      "tslint:recommended"
  ],
  "rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": false,
		"class-name": true,
		"semicolon": [ true, "never"],
		"linebreak-style": [ true, "LF" ],
		"triple-equals": true,
		"no-console": false,
		"object-literal-sort-keys": false
	}
}